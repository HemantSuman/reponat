<%# include ../helper/breadcrumb.ejs %>
<%- include('../helper/Heading-Page.ejs', {title:'Listing'}); %>
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <div class="box-header with-border">
                    
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <form id='searchData' name='searchData' enctype="multipart/form-data" method='get'>
                        <table class="table table-bordered">
                            <tr>
                                <!--<th style="width: 10px">#Id</th>-->
                                <th>Setting Name</th>
                                <th>Setting Value</th>
                                <th style="width: 100px">Actions</th>
                            </tr>

                            <% results.rows.forEach(function(rows){ %>
                                <tr>
                                    <!--<td><%= rows.id %></td>-->
                                    <td><%= rows.setting_name %></td>
                                    <td><%= rows.setting_value %></td>
                                    <td>
                                        <a class="btn btn-success btn-xs" href="/admin/<%= extraVar.viewDirectory %>/edit/<%= rows.id %>" title="Edit" >
                                            <i class="fa fa-edit"></i>
                                        </a>
                                    </td>
                                </tr>
                            <% }); %>
                        </table>
                        <%# include('../element/helper.ejs', {type:'empty_table',data:data}); %>
                    </form>
                </div>
                <!-- /.box-body -->
                <div class="box-footer clearfix">
                    <%- include ../helper/pagination.ejs %>
                </div>
            </div>

        </div>

        <!-- /.col -->
    </div>
</section>