<section class="content">
    <div class="row">
        <div class="col-lg-6 col-xs-6">
            <!-- small box -->
            <div class="small-box bg-aqua">
                <div class="inner">
                    <h3><%= results['total_no'] %></h3>

                    <p>Total Nyaya Card</p>
                </div>
                <div class="icon">
                    <i class="fa fa-shopping-cart"></i>
                </div>
                <a class="small-box-footer" href="/admin/arbitration_agreements/1">
                    More info <i class="fa fa-arrow-circle-right"></i>
                </a>
            </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-6 col-xs-6">
            <!-- small box -->
            <div class="small-box bg-green">
                <div class="inner">
                    <h3>
                        <% if(! membership['total'] || membership['total']==''){ %>    
                        Rs. 0
                        <% }else{ %>
                        Rs. <%= membership['total'] %>
                        <% } %>    

                    </h3>

                    <p>Total <strong><%= membership['count'] %></strong> Membership Associated</p>
                    
                </div>
                <div class="icon">
                    <i class="ion ion-stats-bars"></i>
                </div>
                <a class="small-box-footer" href="/admin/membership_plans/issue_listings">
                    More info <i class="fa fa-arrow-circle-right"></i>
                </a>
            </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-6 col-xs-6">
            <!-- small box -->
            <div class="small-box bg-yellow">
                <div class="inner">
                    <h3>Rs. <%= results['total_fee'] %></h3>

                    <p>Total Cost of Agreement</p>
                </div>
                <div class="icon">
                    <i class="ion ion-person-add"></i>
                </div>
                <a class="small-box-footer" href="/admin/arbitration_agreements/1">
                    More info <i class="fa fa-arrow-circle-right"></i>
                </a>
            </div>
        </div>
        <!-- ./col -->
        <div class="col-lg-6 col-xs-6">
            <!-- small box -->
            <div class="small-box bg-red">
                <div class="inner">
                    <h3>
                        <% if(! results['current_month_fee'] || results['current_month_fee']==''){ %>    
                        Rs. 0
                        <% }else{ %>
                        Rs. <%= results['current_month_fee'] %>
                        <% } %>
                    </h3>

                    <p>Total Cost of Current Month</p>
                </div>
                <div class="icon">
                    <i class="ion ion-pie-graph"></i>
                </div>
                <a class="small-box-footer" href="/admin/arbitration_agreements/1">
                    More info <i class="fa fa-arrow-circle-right"></i>
                </a>
            </div>
        </div>


        <div class="col-lg-6 col-xs-6">
            <div class="box box-info">
                <div class="box-body chart-responsive">
                    <div class="chart" id="line-chart" style="height: 300px;"></div>
                </div>
            </div>
        </div>


        <div class="col-lg-6 col-xs-6">

            <div class="box box-info">

                <table class="table table-bordered">
                    <tr>

                        <th>Stamp No</th>
                        <th>Party Name</th>
                        
                        <th>Mobile No</th>
                        <th>Document Type</th>
                        <th>Status</th>
                    </tr>
                    <% dataArr.forEach(function(rows){ %>
                    <tr>

                        <td><%= rows.stamp_no %></td>
                        <td><%= rows.party_name %></td>
                        
                        <td><%= rows.party_mobile_no %></td>
                        <td><%= rows.master.service_details_name %></td>
                        <td>
                                    <% if(rows.deal_status){ %>              
                                    <span class="label label-success">Mature</span>   
                                    <% }else{ %>
                                    <% if(rows.is_expire){ %>
                                    <span class="label label-danger">Expired</span>
                                    <% }else{ %>
                                    <span class="label label-info">Freezed</span>
                                    <% } %>
                                    <% } %>
                                </td>
                    </tr>
                    <% }); %>    
                </table>

            </div>
        </div>
        <!-- ./col -->
    </div>
</section>
<!-- Morris.js charts -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
<script src="/admin/plugins/morris/morris.min.js"></script>
<script>
    $(function () {
        "use strict";
        // LINE CHART
        var line = new Morris.Line({
            element: 'line-chart',
            resize: true,
            data: [
                {y: '2011-12', item1: 9666},
                {y: '2012-01', item1: 2666},
                {y: '2012-02', item1: 5000},
                {y: '2012-03', item1: 0},
                {y: '2012-04', item1: 0},
                {y: '2012-05', item1: 0},
                {y: '2012-06', item1: 0},
                {y: '2012-07', item1: 0}
            ],
            xkey: 'y',
            ykeys: ['item1'],
            labels: ['Item 1'],
            lineColors: ['#3c8dbc'],
            hideHover: 'auto'
        });
    });
</script>
