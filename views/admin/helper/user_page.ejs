<% if (extraVar.pageCount > 1) {  %>
<ul class="pagination pagination-sm no-margin pull-right">
    <% if (extraVar.currentPage > 1) { %>
    <li><a href="/admin/<%= extraVar.viewDirectory %>/<%= extraVar.role_id %>?page=<%= extraVar.pre %>" class="changePage">&laquo;</a></li>
    <% } %>
    <% var i = 1;
    if (extraVar.currentPage > 5) {
    i = +extraVar.currentPage - 4;
    } %>
    <% if (i !== 1) { %>
    <!--<li class="disabled"><a href="#">...</a></li>-->
    <% } %>
    <% for (i; i<=extraVar.pageCount; i++) { %>
    <% if (extraVar.currentPage == i) { %>
    <li class="active"><span><%= i %> <span class="sr-only">(current)</span></span></li>
    <% } else { %>
    <li><a href="/admin/<%= extraVar.viewDirectory %>/<%= extraVar.role_id %>?page=<%= i %><%= extraVar['searchStr'] %>" class="changePage"><%= i %></a></li>
    <% } %>
    <% if (i == (+extraVar.currentPage + 4)) { %>
    <!--<li class="disabled"><a href="#">...</a></li>-->
    <% break; } %>
    <% } %>
    <% if (extraVar.currentPage != extraVar.pageCount) { %>
    <li><a href="/admin/<%= extraVar.viewDirectory %>/<%= extraVar.role_id %>?page=<%= extraVar.next %>" class="changePage">&raquo;</a></li>
    <% } %>
</ul>
<% } %>
