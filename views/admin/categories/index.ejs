<%# include ../helper/breadcrumb.ejs %>

<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title"> </h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">

                    <form id='searchData' name='searchData' enctype="multipart/form-data" method='post'>
                        <table class="table table-bordered">
                            <tr>
                                <th style="width: 10px">#Id</th>
                                <th>Name</th>
                                <th>Status</th>
                                <th style="width: 100px">Actions</th>
                            </tr>
  

                            <% results.forEach(function(rows){ %>
                            <tr>
                                <td><%= rows.id %></td>
                                <td><%= rows.category_name %></td>
                                <td>
                                    <% if(rows.is_active==1){ %>
                                    <span class="label label-success">Active</span>                               
                                    <% }else{ %>
                                    <span class="label label-danger">Inactive</span>
                                    <% } %>

                                </td>
                                <td>

                                    <a class="btn btn-success btn-xs" href="/admin/users/edit/<%= rows.id %>" title="Edit" >
                                        <i class="fa fa-edit"></i></a>

                                    <a class="btn btn-success btn-xs blue" href="#" title="Reset Password"  ><i class="fa fa-edit"></i></a>

                                    <a class="btn btn-danger btn-xs" href="#" ><i class="fa <%= (rows.is_active==1)? 'fa-lock':'fa-unlock' %>"></i></a>
                                </td>

                            </tr>
                            <% }); %>


                        </table>
                        <%- include('../element/no-record.ejs', {results:results}); %>
                    </form>
                </div>
                <!-- /.box-body -->
                <div class="box-footer clearfix">

                    <%# include ../helper/page2.ejs %>




                </div>
            </div>

        </div>

        <!-- /.col -->
    </div>
</section>