
<div class="panel panel-default">
    <div class="panel-heading">Document Information</div>
    <div class="panel-body">
        <div class="parties">
            <div class="position_relative party_group" id="">
                <div class="position_relative party_content" id="">
                    <div class="panel panel-default" id="">
                        <div class="panel-body panel-grey">

                            <div class="row form-row">
                                <div class="col-md-3 col-xs-12">
                                    <label> Document Stamp Number <em>*</em></label>
                                    <input type="text" name="stamp_number" id="stamp_number" class="inputtxt emptyValidation">
                                    <label class="errorMsg" id="stamp_number_error"></label>
                                </div>
                                <div class="col-md-3 col-xs-12">
                                    <label> Document Category<em>*</em> </label>
                                    <input type="hidden" value="" id="service_details_primary_id" name="service_details_primary_id" >
                                    <span class="settingValueForMonthlyRent hide"><%= settingData.document_category_for_monthly_rent %></span>
                                    <span class="settingValueForPopUp hide"><%= settingData.document_category_for_popup %></span>
                                    <select  name="service_details_id" id="service_details_id" class="  inputtxt emptyValidation"
                                             formType="agreement" onChange="getDbValuesByIds(this, 'ServiceDetailDocument', ['service_detail_id'], [this.options[this.selectedIndex].getAttribute('primaryId')], 'documents_name asc', 'service_detail_documents_id', ['id', 'documents_name'], []);"
                                             >
                                        <option value="">--Please Select--</option>
                                        <% for(var i=0; i < data.service_details.length; i++) { %>
                                        <option service_detail_slug="<%= data.service_details[i]['service_details_slug'] %>" primaryId="<%= data.service_details[i]['id'] %>" value="<%= data.service_details[i]['service_details_name'] %>" ><%= data.service_details[i]['service_details_name'] %></option>
                                        <% } %>
                                    </select>
                                    <label class="errorMsg" id="service_details_id_error"></label>
                                </div>
                                <div class="col-md-3 col-xs-12">
                                    <label> Document Type<em>*</em> </label>
                                    <select  name="service_detail_documents_id" id="service_detail_documents_id" class="  inputtxt emptyValidation">
                                        <option value="">--Please Select--</option>
                                    </select>
                                    <label class="errorMsg" id="service_detail_documents_id_error"></label>
                                </div>

                                <div class="col-md-3 col-xs-12">
                                    <label> Document Executed On <em>*</em></label>
                                    <input type="text" name="stamp_date" id="stamp_date" class="inputtxt emptyValidation">
                                    <label class="errorMsg" id="stamp_date_error"></label>
                                </div>
                            </div>

                        </div>
                    </div>
                    <div class="panel-heading no-left-padd no-top-padd"><strong>Document Executed At</strong></div>
                    <div class="panel panel-default" >
                        <div class="panel-body">
                            <div class="row form-row">
                                <div class="col-md-3 col-xs-12">
                                    <label>State<em>*</em></label>
                                    <input type="hidden" value="" id="stamp_state_id_primary_id" name="stamp_state_primary_id" >
                                    <select name="stamp_state_id" id="stamp_state_id" class="  inputtxt emptyValidation onChangeForPrimaryId"
                                            formType="agreement" onChange="getDbValuesByIds(this, 'Division', ['state_id', 'is_active'], [this.options[this.selectedIndex].getAttribute('primaryId'), 1], 'division_name asc', 'stamp_division_id', ['id', 'division_name'], ['stamp_district_id', 'stamp_tehsil_id']);"
                                            >
                                        <option value="">States</option>
                                        <% for(var i=0; i < data.states.length; i++) { %>
                                        <option primaryId="<%= data.states[i]['id'] %>" value="<%= data.states[i]['state_name'] %>" >
                                            <%= data.states[i]['state_name'] %>
                                        </option>
                                        <% } %>
                                    </select>
                                    <label class="errorMsg" id="stamp_state_id_error"></label>
                                </div>
                                <div class="col-md-3 col-xs-12">
                                    <label>Division<em>*</em></label>
                                    <input type="hidden" value="" id="stamp_division_id_primary_id" name="stamp_division_primary_id" >
                                    <select name="stamp_division_id" id="stamp_division_id" class="  inputtxt emptyValidation onChangeForPrimaryId"
