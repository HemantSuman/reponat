                                        <option value="">District</option>
                                    </select>
                                    <label class="errorMsg" id="stamp_district_id_error"></label>
                                </div>

                                <div class="col-md-3 col-xs-12">
                                    <label> Town/Kasba <em>*</em></label>
                                    <input type="text" name="stamp_town" id="stamp_town" class="inputtxt emptyValidation" >
                                    <label class="errorMsg" id="stamp_town_error"></label>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="panel panel-default" >
                        <div class="panel-body panel-grey">
                            <div class="row form-row">
                                <div class="col-md-3 col-xs-12">
                                    <label><span class="document_value_month_class">Document Value</span><em>*</em></label>
                                    <input type="text" name="agreement_value" id="agreement_value" class="inputtxt emptyValidation" >
                                    <label class="errorMsg" id="agreement_value_error"></label>
                                </div>
                                <div class="col-md-3 col-xs-12">
                                    <label>Document Valid From<em>*</em></label>
                                    <input type="text" name="document_valid_from" id="document_valid_from" class="inputtxt emptyValidation">
                                    <label class="errorMsg" id="document_valid_from_error"></label>
                                </div>
                                <div class="col-md-3 col-xs-12">
                                    <label>Document Valid Up To<em>*</em></label>
                                    <input type="text" name="document_valid_to" id="document_valid_to" class="inputtxt emptyValidation">
                                    <label class="errorMsg" id="document_valid_to_error"></label>
                                </div>
                                <div class="col-md-3 col-xs-12">
                                    <label>Select Place of Arbitration<em>*</em></label>
                                    <input type="hidden" value="" id="place_of_arbitration_primary_id" name="place_of_arbitration_primary_id" >
                                    <select name="place_of_arbitration" id="place_of_arbitration" class="inputtxt emptyValidation onChangeForPrimaryId" >
                                        <option value="" >--Please Select--</option>
                                        <% for(var i=0; i < data.states_is_business.length; i++) { %>
                                        <option primaryId="<%= data.states_is_business[i]['id'] %>" value="<%= data.states_is_business[i]['state_name'] %>" >
                                            <%= data.states_is_business[i]['state_name'] %>
                                        </option>
                                        <% } %>
                                    </select>
                                    <label class="errorMsg" id="place_of_arbitration_error"></label>
                                </div>
                                <div class="col-md-6 col-xs-12 twoTeirDiv" style="display: none;">
                                    <label>Select Place of Appeal (Second Arbitration)<em>*</em></label>
                                    <input type="hidden" value="" id="place_of_appeal_primary_id" name="place_of_appeal_primary_id" >
                                    <select name='place_of_appeal' id='place_of_appeal' class="inputtxt onChangeForPrimaryId" >
                                        <option value="" >--Please Select--</option>
                                        <% for(var i=0; i < data.states_is_business.length; i++) { %>
                                        <option primaryId="<%= data.states_is_business[i]['id'] %>" value="<%= data.states_is_business[i]['state_name'] %>" >
                                            <%= data.states_is_business[i]['state_name'] %>
                                        </option>
                                        <% } %>
                                    </select>
                                    <label class='errorMsg' id='place_of_appeal_error'></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

