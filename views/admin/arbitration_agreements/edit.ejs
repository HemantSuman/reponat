<%- include('../helper/Heading-Page.ejs', {title:''}); %>

<section class="content arbitForm">
    <div class="box box-primary">
        <div class="filterBar">
            <div class="filterBarRight"> <a href="/admin/<%= extraVar.viewDirectory %>/<%= extraVar.product_id %>" class="btn-view-list"><i class="fa fa-list" aria-hidden="true"></i> View Listing</a> </div>
        </div>
        <div class="box-body">
            <div id="formWizard">
                <div  style="display: none;" class="navbar">
                    <div class="navbar-inner">
                        <ul>
                            <li><a href="#tab1" id="tab1Li" data-toggle="tab">Step 1</a></li>
                            <!--tabStatus="second" destinationId="tab2" modelNames="countries" renderTemplate="admin/arbitration_agreements/view_detail" rendringType="htmlType" url="/admin/loadAgreementTypeContent"-->
                            <li><a href="#tab2" id="tab2Li" data-toggle="tab">Step 2</a></li>
                            <li><a  href="#tab3" id="tab3Li" data-toggle="tab">Step 3</a></li>
                            <li><a  href="#tab4" id="tab4Li" data-toggle="tab">Step 4</a></li>
                        </ul>
                    </div>
                </div>
                <div class="tab-content">
                    <!-- tab 1 -->
                    <div class="tab-pane" id="tab1">
                        <form role="form" id="formAgreement" name='' method='post' action='/admin/<%= extraVar.viewDirectory %>/proceed_to_view' enctype="multipart/form-data">
                            <input type="hidden" name='id' value='<%= results.id %>'>
                            <input type="hidden" name='product_id' value='<%= extraVar.product_id %>'>
                            <input type="hidden" name='user_id' value='<%= results.user_id %>'>
                            <!--<input name="image_path" type="hidden" value="public/uploads/signature">-->
                            <div class="row form-row-new">
                                <div class="col-md-12">
                                    <!--<h4></h4>-->
                                    <h5 class="text-danger">* marked fields are mandatory.</h5>
                                    <br>
                                    <div class="form-group">
                                        <div class="chk-outer">
                                            <input type="checkbox" name="term_conditions" id="term_conditions" class="emptyValidation"  <% if(results.term_conditions == 1){ %> checked <% } %> >
                                                   <label for="term_conditions"> <span>By creating Arbitration Agreement , you are agreeing <a target="_blank" href="/pages/terms-condition">terms and conditions</a> and you want to create Nyaya card with us<em>*</em></span> </label>
                                            <br>
                                            <label class="errorMsg" id="term_conditions_error"></label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="radio-btn">
                                            <input type="radio" rendringType='htmlType' renderTemplate='admin/arbitration_agreements/default_member_form' modelNames="countries,states,service_details,states_is_business" destinationId='agreementTypeDiv' url="/common/loadAgreementTypeContent" class="agreementType1 agreementType" name="agreement_type" ID="singletier" value="single" <% if(results.agreement_type == 'single'){ %> checked <% } %> >
                                                   <label for="singletier"> Single Tier </label>
                                            <input type="radio" rendringType='htmlType' renderTemplate='admin/arbitration_agreements/default_member_form' modelNames="countries,states,service_details,states_is_business" destinationId='agreementTypeDiv' url="/common/loadAgreementTypeContent" class="agreementType2 agreementType" name="agreement_type"  id="twotier" value="two" <% if(results.agreement_type == 'two'){ %> checked <% } %> >
                                                   <label for="twotier"> Two Tier </label>
                                        </div>
                                    </div>
                                    <div class="form-group col2"> <span>This arbitration agreement made on this <strong> <%= site.momentObj(Date.now()).format('DD-MM-YYYY HH:MM:ss') %> </strong>at
                                            <div>
                                                <label class="doNotRemove---"></label>
                                                <input type="text" name="agreement_place" id="agreement_place" value="<%= results.agreement_place %>" class="inputtxt emptyValidation" placeholder="enter City/Town/Kasba">
                                                <label class="errorMsg" id="agreement_place_error"></label>
                                            </div>
                                        </span> </div>
                                </div>
                            </div>
                            <div id="agreementTypeDiv">
                                <div class="panel panel-default">
                                    <div class="panel-heading">Document Information</div>
                                    <div class="panel-body">
                                        <div class="">
                                            <div class="position_relative party_group" id="">
                                                <div class="position_relative party_content" id="">
                                                    <div class="panel panel-default" id="">
                                                        <div class="panel-body panel-grey">
                                                            <div class="row">
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label> Document Stamp Number <sup class="text-danger">*</sup></label>
                                                                    <input type="text" value="<%= results.stamp_number %>" name="stamp_number" id="stamp_number" class="inputtxt emptyValidation" placeholder="Enter Stamp Number">
                                                                    <label class="errorMsg" id="stamp_number_error"></label>
                                                                </div>
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label> Document Category<sup class="text-danger">*</sup> </label>
                                                                    <input type="hidden" value="<%= results.service_details_primary_id %>" id="service_details_primary_id" name="service_details_primary_id" >
                                                                    <span class="settingValueForMonthlyRent hide"><%= settingData.document_category_for_monthly_rent %></span>
                                                                    <span class="settingValueForPopUp hide"><%= settingData.document_category_for_popup %></span>
                                                                    <select  name="service_details_id" id="service_details_id" class=" inputtxt emptyValidation"
                                                                             formType="agreement" onChange="getDbValuesByIds(this, 'ServiceDetailDocument', ['service_detail_id'], [this.options[this.selectedIndex].getAttribute('primaryId')], 'documents_name asc', 'service_detail_documents_id', ['id', 'documents_name'], []);"
                                                                             >
                                                                        <option value="">--Please Select--</option>
                                                                        <% for(var i=0; i < data.service_details.length; i++) { %>
                                                                        <option <% if(data.service_details[i]['id'] == results.service_details_primary_id){%> selected <% } %> service_detail_slug="<%= data.service_details[i]['service_details_slug'] %>" primaryId="<%= data.service_details[i]['id'] %>" value="<%= data.service_details[i]['service_details_name'] %>" ><%= data.service_details[i]['service_details_name'] %></option>
                                                                        <% } %>
                                                                    </select>
                                                                    <label class="errorMsg" id="service_details_id_error"></label>
                                                                </div>
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label> Document Type<sup class="text-danger">*</sup> </label>
                                                                    <select  name="service_detail_documents_id" id="service_detail_documents_id" class=" inputtxt emptyValidation">
                                                                        <option value="">--Please Select--</option>
                                                                        <% for(var i=0; i < data.service_detail_documents.length; i++) { %>
                                                                        <option <% if(data.service_detail_documents[i]['documents_name'] == results.service_detail_documents_id){%> selected <% } %>  primaryId="<%= data.service_detail_documents[i]['id'] %>" value="<%= data.service_detail_documents[i]['documents_name'] %>" ><%= data.service_detail_documents[i]['documents_name'] %></option>
                                                                        <% } %>
                                                                    </select>
                                                                    <label class="errorMsg" id="service_detail_documents_id_error"></label>
                                                                </div>
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label> Document Executed On <sup class="text-danger">*</sup></label>
                                                                    <input type="text" name="stamp_date" id="stamp_date" value="<% if(results.document_valid_to){ %> <%= site.momentObj(results.stamp_date).format('DD-MM-YYYY') %> <% } %>" class="inputtxt emptyValidation">
                                                                    <label class="errorMsg" id="stamp_date_error"></label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="panel-heading no-left-padd no-top-padd"><strong>Document Executed At</strong></div>
                                                    <div class="panel panel-default" id="">
                                                        <div class="panel-body">
                                                            <div class="row">
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label>State<sup class="text-danger">*</sup></label>
                                                                    <input type="hidden" value="<%= results.stamp_state_primary_id %>" id="stamp_state_id_primary_id" name="stamp_state_primary_id" >
                                                                    <select name="stamp_state_id" id="stamp_state_id" class=" inputtxt emptyValidation onChangeForPrimaryId"
                                                                            formType="agreement" onChange="getDbValuesByIds(this, 'Division', ['state_id', 'is_active'], [this.options[this.selectedIndex].getAttribute('primaryId'), 1], 'division_name asc', 'stamp_division_id', ['id', 'division_name'], ['stamp_district_id', 'stamp_tehsil_id']);"
                                                                            >
                                                                        <option value="">States</option>
                                                                        <% for(var i=0; i < data.stamp_states.length; i++) { %>
                                                                        <option <% if(data.stamp_states[i]['id'] == results.stamp_state_primary_id){ %> selected <% } %> primaryId="<%= data.stamp_states[i]['id'] %>" value="<%= data.stamp_states[i]['state_name'] %>" > <%= data.stamp_states[i]['state_name'] %> </option>
                                                                        <% } %>
                                                                    </select>
                                                                    <label class="errorMsg" id="stamp_state_id_error"></label>
                                                                </div>
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label>Division<sup class="text-danger">*</sup></label>
                                                                    <input type="hidden" value="<%= results.stamp_division_primary_id %>" id="stamp_division_id_primary_id" name="stamp_division_primary_id" >
                                                                    <select name="stamp_division_id" id="stamp_division_id" class=" inputtxt emptyValidation onChangeForPrimaryId"
                                                                            formType="agreement" onChange="getDbValuesByIds(this, 'District', ['division_id', 'is_active'], [this.options[this.selectedIndex].getAttribute('primaryId'), 1], 'district_name asc', 'stamp_district_id', ['id', 'district_name'], ['stamp_tehsil_id']);"
                                                                            >
                                                                        <option value="">Division</option>
                                                                        <% for(var i=0; i < data.stamp_divisions.length; i++) { %>
                                                                        <option <% if(data.stamp_divisions[i]['id'] == results.stamp_division_primary_id){ %> selected <% } %> primaryId="<%= data.stamp_divisions[i]['id'] %>" value="<%= data.stamp_divisions[i]['division_name'] %>" > <%= data.stamp_divisions[i]['division_name'] %> </option>
                                                                        <% } %>
                                                                    </select>
                                                                    <label class="errorMsg" id="stamp_division_id_error"></label>
                                                                </div>
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label>District<sup class="text-danger">*</sup></label>
                                                                    <input type="hidden" value="<%= results.stamp_district_primary_id %>" id="stamp_district_id_primary_id" name="stamp_district_primary_id" >
                                                                    <select name="stamp_district_id" id="stamp_district_id" class=" inputtxt emptyValidation onChangeForPrimaryId">
                                                                        <option value="">District</option>
                                                                        <% for(var i=0; i < data.stamp_district.length; i++) { %>
                                                                        <option <% if(data.stamp_district[i]['id'] == results.stamp_district_primary_id){ %> selected <% } %> primaryId="<%= data.stamp_district[i]['id'] %>" value="<%= data.stamp_district[i]['district_name'] %>" > <%= data.stamp_district[i]['district_name'] %> </option>
                                                                        <% } %>
                                                                    </select>
                                                                    <label class="errorMsg" id="stamp_district_id_error"></label>
                                                                </div>
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label> Town/Kasba <sup class="text-danger">*</sup></label>
                                                                    <input type="text" value="<%= results.stamp_town %>" name="stamp_town" id="stamp_town" class="inputtxt emptyValidation" placeholder="Enter Place">
                                                                    <label class="errorMsg" id="stamp_town_error"></label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="panel panel-default" id="">
                                                        <div class="panel-body panel-grey">
                                                            <div class="row">
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label><span class="document_value_month_class"><% if(locals.settingData.document_category_for_monthly_rent.split(',').indexOf(results.service_details_id) >= 0){ %>Monthly Lease Rent<% } else { %>Document Value<% } %></span><sup class="text-danger">*</sup></label>
                                                                    <input type="text" value="<%= results.agreement_value %>" name="agreement_value" id="agreement_value" class="inputtxt emptyValidation" placeholder="Document Value">
                                                                    <label class="errorMsg" id="agreement_value_error"></label>
                                                                </div>
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label>Document Valid From<sup class="text-danger">*</sup></label>
                                                                    <input type="text" value="<% if(results.document_valid_to){ %> <%= site.momentObj(results.document_valid_from).format('DD-MM-YYYY') %> <% } %>" name="document_valid_from" id="document_valid_from" class="inputtxt emptyValidation" placeholder="Document Valid Form">
                                                                    <label class="errorMsg" id="document_valid_from_error"></label>
                                                                </div>
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label>Document Valid Up To<sup class="text-danger">*</sup></label>
                                                                    <input type="text" value="<% if(results.document_valid_to){ %> <%= site.momentObj(results.document_valid_to).format('DD-MM-YYYY') %> <% } %>" name="document_valid_to" id="document_valid_to" class="inputtxt emptyValidation" placeholder="Document Valid To">
                                                                    <label class="errorMsg" id="document_valid_to_error"></label>
                                                                </div>
                                                                <div class="col-md-3 col-xs-12">
                                                                    <label>Select Place of Arbitration<sup class="text-danger">*</sup></label>

                                                                    <input type="hidden" value="<%= results.place_of_arbitration_primary_id %>" id="place_of_arbitration_primary_id" name="place_of_arbitration_primary_id" >
                                                                    <select name="place_of_arbitration" id="place_of_arbitration" class="inputtxt onChangeForPrimaryId" >
                                                                        <option value="" >--Please Select--</option>
                                                                        <% for(var i=0; i < data.states_is_business.length; i++) { %>
                                                                        <option <% if(data.states_is_business[i]['id'] == results.place_of_arbitration_primary_id){ %> selected <% } %> primaryId="<%= data.states_is_business[i]['id'] %>" value="<%= data.states_is_business[i]['state_name'] %>" >
                                                                            <%= data.states_is_business[i]['state_name'] %>
                                                                    </option>
                                                                    <% } %>
                                                                </select>
                                                                <label class="errorMsg" id="place_of_arbitration_error"></label>
                                                            </div>
                                                            <% if(results.agreement_type == 'two'){ %>
                                                            <div class="col-md-4 col-xs-12 twoTeirDiv"  >
                                                                <label>Select Place of Appeal (Second Arbitration)<sup class="text-danger">*</sup></label>
                                                                <input type="hidden" value="<%= results.place_of_appeal_primary_id %>" id="place_of_appeal_primary_id" name="place_of_appeal_primary_id" >
                                                                <select name='place_of_appeal' id='place_of_appeal' class="inputtxt onChangeForPrimaryId">
                                                                    <option value="">Please Select</option>
                                                                    <% for(var i=0; i < data.states_is_business.length; i++) { %>
                                                                    <option <% if(data.states_is_business[i]['id'] == results.place_of_appeal_primary_id){ %> selected <% } %> primaryId="<%= data.states_is_business[i]['id'] %>" value="<%= data.states_is_business[i]['state_name'] %>" >
                                                                        <%= data.states_is_business[i]['state_name'] %>
                                                                </option>
                                                                <% } %>
                                                            </select>
                                                            <label class='errorMsg' id='place_of_appeal_error'></label>
                                                        </div>
                                                        <% } else { %>
                                                        <div class="col-md-4 col-xs-12 twoTeirDiv"  style="display: none;" >
                                                            <label>Select Place of Appeal (Second Arbitration)<sup class="text-danger">*</sup></label>
                                                            <input type="hidden" disabled="disabled" value="<%= results.place_of_appeal_primary_id %>" id="place_of_appeal_primary_id" name="place_of_appeal_primary_id" >
                                                            <select disabled="disabled" name='place_of_appeal' id='place_of_appeal' class="inputtxt onChangeForPrimaryId">
                                                                <option value="">Please Select</option>
                                                                <% for(var i=0; i < data.states_is_business.length; i++) { %>
                                                                <option primaryId="<%= data.states_is_business[i]['id'] %>" value="<%= data.states_is_business[i]['state_name'] %>" >
                                                                    <%= data.states_is_business[i]['state_name'] %>
                                                                </option>
                                                                <% } %>
                                                            </select>
                                                            <label class='errorMsg' id='place_of_appeal_error'></label>

                                                        </div>
                                                        <%} %>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="parties" id="parties">
                            <% var party_count = 0; %>
                            <% Object.keys(formValue).forEach(function(party, key){ %>
                            <% var member_count = 0; %>
                            <% party_count++ %>
                            <div class="panel panel-default position_relative" id="parties_<%= party %>">
                                <div class="panel-heading">Party No. <span class="partyNoTitleCount"><%= party %></span></div>
                                <div class="panel-body"> <a party_no="2" href="javascript:void(0);" class="closeParty partyCloseDivClass" ><i class="fa fa-times-circle-o" aria-hidden="true"></i></a>
                                    <div class="position_relative party_content" id="party_content_<%= party %>">
                                        <% formValue[party].forEach(function(members){ %>
                                        <% member_count++ %>
                                        <div id="member_content_<%= party %>_<%= members.member_no %>" class="member_content_class"> <a party_no="<%=party%>" member_no="<%=members.member_no%>" href="javascript:void(0);" class="closeMember memberCloseDivClass_<%=party%>_<%=members.member_no%>" ><i class="fa fa-times-circle-o" aria-hidden="true"></i></a>
                                            <div class="panel panel-default">
                                                <div class="panel-body panel-grey">
                                                    <div class="row">
                                                        <div class="col-sm-12">
                                                            <input type="hidden" name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][arbitration_agreement_party_number]" value="<%= party %>" >
                                                            <input type="hidden" name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][party_no]" value="<%= party %>" >
                                                            <input type="hidden" name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][member_no]" value="<%= members.member_no %>" >
                                                            <div class="radio-btn">
                                                                <input name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][user_type]" type="radio" class="userTypeCls" userTypeAlias='individual' value='individual' party_no='<%= party %>' member_no='<%= members.member_no %>' <% if(members.user_type == 'individual' || !members.user_type){ %> checked="checked"  <% } %> id="individual<%= party %><%= members.member_no %>"/>
                                                                       <label for="individual<%= party %><%= members.member_no %>">Individual</label>
                                                                <input name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][user_type]" type="radio" class="userTypeCls" userTypeAlias='behalf_of_company' value='behalf_of_company' party_no='<%= party %>' member_no='<%= members.member_no %>' <% if(members.user_type == 'behalf_of_company'){ %> checked="checked"  <% } %> id="comp<%= party %><%= members.member_no %>"/>
                                                                       <label for="comp<%= party %><%= members.member_no %>">Behalf of Company or Firm </label>
                                                                <input name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][user_type]" type="radio" class="userTypeCls" userTypeAlias='power_of_attorney_holder' value='power_of_attorney_holder' party_no='<%= party %>' member_no='<%= members.member_no %>' <% if(members.user_type == 'power_of_attorney_holder'){ %> checked="checked"  <% } %> id="Attor<%= party %><%= members.member_no %>" />
                                                                       <label for="Attor<%= party %><%= members.member_no %>">Power of Attorney Holder </label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row userTypeBehalf_<%= party %>_<%= members.member_no %>">
                                                        <% if(members.user_type == 'behalf_of_company'){ %>
                                                        <div class="col-md-8 col-xs-12"><label> Business Name </label><input name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][business_name]" id="member_name_<%= party %>_<%= members.member_no %>" type="text" value="<%= members.business_name %>" class="inputtxt emptyValidation" placeholder="complete name of business, you would like to mention on agreement">
                                                            <label class="errorMsg" id="business_name_party_no_member_no_error"></label>

                                                        </div>

                                                        <div class="col-md-4 col-xs-12">
                                                            <label> Through it's (Designation)</label>
                                                            <input name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][designation]" id="Designation_<%= party %>_<%= members.member_no %>" value="<%= members.designation %>" type="text" class="inputtxt emptyValidation" placeholder="Director/ Partner/ Proprietor/ manager etc.">
                                                            <label class="errorMsg" id="Designation_party_no_member_no_error"></label>
                                                        </div>
                                                        <% } %>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-1 col-xs-12">
                                                            <label class="labeled name_of_party_member_title_<%= party %>_<%= members.member_no %>">
                                                                <% if(members.user_type == 'behalf_of_company'){ %>
                                                                Name of Designated person
                                                                <% } else if(members.user_type == 'power_of_attorney_holder'){ %>
                                                                Power of Attorney Holder
                                                                <% } else { %>
                                                                Name of party
                                                                <% } %>
                                                                <sup class="text-danger">*</sup></label>
                                                            <select name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][member_title]" id="member_title_<%= party %>_<%= members.member_no %>" class="inputtxt emptyValidation">
                                                                <option <% if(members.member_title == 'Mr.'){ %> selected <%}%> value="Mr.">Mr.</option>
                                                                <option <% if(members.member_title == 'Mrs.'){ %> selected <%}%> value="Mrs.">Mrs.</option>
                                                                <option <% if(members.member_title == 'Miss.'){ %> selected <%}%> value="Miss.">Miss.</option>
                                                                <option value="Ms.">Ms.</option>
                                                            </select>
                                                        </div>
                                                        <div class="col-md-3 col-xs-12">
                                                            <label> &nbsp; </label>
                                                            <input value="<%= members.member_name %>" name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][member_name]" id="member_name_<%= party %>_<%= members.member_no %>" type="text" class="inputtxt emptyValidation" placeholder="Party Full Name">
                                                            <label class="errorMsg" id="member_name_<%= party %>_<%= members.member_no %>_error"></label>
                                                        </div>
                                                        <div class="col-md-3 col-xs-12">
                                                            <!--<label> Father's/Husband Name </label>-->
                                                            <label>
                                                                <input <% if(members.member_relation_of == 'S/O'){ %> checked <% } %> name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][member_relation_of]" type="radio" class="" value="S/O" >
                                                                    S/O
                                                                    <input <% if(members.member_relation_of == 'W/O'){ %> checked <% } %> name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][member_relation_of]" type="radio" class="" value="W/O" >
                                                                    W/O
                                                                    <input <% if(members.member_relation_of == 'D/O'){ %> checked <% } %> name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][member_relation_of]" type="radio" class="" value="D/O" >
                                                                    D/O </label>
                                                            <input value="<%= members.member_relation_name %>" name="arbitration_agreement_party_members[<%= party %>][<%= members.member_no %>][member_relation_name]" id="member_relation_name_<%= party %>_<%= members.member_no %>" type="text" class="inputtxt emptyValidation" placeholder="Party father or husband Name">
                                                            <label class="errorMsg" id="member_relation_name_<%= party %>_<%= members.member_no %>_error"></label>
                                                        </div>
                                                        <div class="col-md-2 col-xs-12">
