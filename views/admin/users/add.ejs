                                        <option value="<%= value.id %>" <% if(fromData.role_type_id==value.id){ %>selected<% } %> ><%= value.role_type_name %></option>
                                        <% });  %>
                                    </select>
                                    <label class="error_mgs_lable" id="role_type_id_error"></label>
                                </div>
                            </div>
                            <% } %>
