<%# include ../helper/breadcrumb.ejs %>
<%- include('../helper/Heading-Page.ejs', {title: fromData.id ? 1 == extraVar.view_type? "View" : "Edit" : "Add" }); %>

<section class="content">
    <div class="box box-primary">
