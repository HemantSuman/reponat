<%- include('../helper/Heading-Page.ejs', {title:''}); %>
<section class="content">
    <div class="box box-primary">
        <div class="filterBar">
            <div class="filterBarRight">
                
            </div>
        </div>
        <!-- /.box-header --> 
        <!-- form start -->
        <form role="form" id='formSubmit' name='' method='post' action='/admin/<%= extraVar.viewDirectory %>/change-password' enctype="multipart/form-data">
            <div class="box-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Old Password<em>*</em></label>
                            <input type="password" class="form-control" id="old_password" name='old_password' >
                            <label class="error_mgs_lable" id="old_password_error"></label>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label for="exampleInputEmail1">New Password<em>*</em></label>
                            <input type="password" class="form-control" id="password" name='password' >
                            <label class="error_mgs_lable" id="password_error"></label>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Confirm New Password<em>*</em></label>
                            <input type="password" class="form-control" id="new_confirm_password" name='new_confirm_password' >
                            <label class="error_mgs_lable" id="new_confirm_password_error"></label>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.box-body -->
            <div class="box-footer">
                <div class="row">
                    <div class="col-md-12 text-right">
                      <%# include('../helper/commonHtml.ejs', {htmlType:'SaveCancel'}); %>
                      <button type="submit" class="btn btn-primary">Save</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</section>

